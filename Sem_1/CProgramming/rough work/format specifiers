#include <stdio.h>

int main ()
{
printf("%%#x: %#x\n", 120 );

printf("%%x: %x\n", 12 );

printf("%%#X: %#X\n", 120 );

printf("%%X: %X\n", 12 );

printf("%%#o: %#o\n", 120 );

printf("%%o: %o\n", 12 );

printf("%%#2f: %#2f\n", 120.567 );

printf( "%%g: %g\n", 333.1415926 );

printf( "%%g: %g\n", 93000000.0 );

printf( "%%G: %e\n", 93000000.0 );

printf("%%07d: %07d\n", 102 );

printf("%+d\n", 102 );

printf("%%-7d,%%-5d,: %-7d,%-5d,\n", 11, 22 );

printf("%%#010x: %#010x\n", 121 );

printf("%%#010X: %#010X\n", 121 );

printf("%ls\n", "my name");

printf("%Lg\n", 45.23456123);

printf("%.0d\n", 6);

printf("%.0d\n", 0);

printf("%%4f: %4f\n", 12.4321 );

printf( "%%8.5f: %8.5f\n", 1.234 );

printf("%%.1f: %4.1f\n", 12.4321 );

printf("%%.3f: %.3f\n", 12.4321 );

printf( "%%.3f: %.3f\n%%.3g: %.3g\n%%.3f:%.3f\n%%.3g: %.3g\n", 100.2, 100.2,3.1415926, 3.1415926 );

printf( "%%.5s: %.5s\n", "abcdefg" );

printf( "%%g: %g\n", 93010000.0 );
}
